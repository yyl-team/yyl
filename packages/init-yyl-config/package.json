{
  "version": "0.1.3",
  "main": "output/index.js",
  "types": "output/index.d.ts",
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": [
      "yyl-react"
    ]
  },
  "eslintIgnore": [
    "/node_modules",
    "/output",
    "/test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/yyl.git"
  },
  "bugs": {
    "url": "https://github.com/jackness1208/yyl/issues"
  },
  "homepage": "https://github.com/jackness1208/yyl#readme",
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "printWidth": 100
  },
  "scripts": {
    "bootstarp": "yarn install & npm run prettier",
    "eslint": "eslint --ext=jsx,ts,tsx ./",
    "prettier": "prettier --write ./**",
    "dev": "rollup --config ./rollup.config.js",
    "d": "rollup --config ./rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup --config ./rollup.config.js",
    "w": "rollup --watch --config ./rollup.config.js",
    "prepublishOnly": "npm run prettier & npm run build & npm test & npm run doc",
    "doc": "typedoc ./src/index.ts",
    "test": "jest ./test/jest/"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-yyl-react": "^0.1.9",
    "jest": "26.6.3",
    "prettier": "^2.0.4",
    "rollup": "^2.38.2",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "typedoc": "^0.20.20",
    "typescript": "^4.1.3"
  },
  "name": "init-yyl-config",
  "description": "init yyl config",
  "author": "liudaojie <liudaojie@yy.com>",
  "license": "MIT",
  "dependencies": {
    "yyl-config-types": "^0.4.0"
  }
}
