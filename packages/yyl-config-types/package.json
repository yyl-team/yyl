{
  "name": "@yylive/config-types",
  "version": "0.1.0",
  "description": "yyl.config types",
  "main": "output/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/yyl.git"
  },
  "bugs": {
    "url": "https://github.com/jackness1208/yyl/issues"
  },
  "homepage": "https://github.com/jackness1208/yyl#readme",
  "scripts": {
    "eslint": "eslint --ext=jsx,ts,tsx ./",
