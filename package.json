{
  "name": "yyl",
  "version": "3.9.0",
  "description": "yy live workflow",
  "keywords": [
    "yylive"
  ],
  "preferGlobal": true,
  "author": "yylive FE team",
  "license": "ISC",
  "bin": {
    "yyl": "bin/init.js"
  },
  "scripts": {
    "test": "mocha ./test/mocha",
    "test:all": "mocha ./test/mocha/all.test.js",
    "test:help": "mocha ./test/mocha/help.test.js",
    "test:info": "mocha ./test/mocha/info.test.js",
    "test:init": "mocha ./test/mocha/init.test.js",
    "test:mock": "mocha ./test/mocha/mock.test.js",
    "test:path": "mocha ./test/mocha/path.test.js",
    "test:server": "mocha ./test/mocha/server.test.js",
    "test:version": "mocha ./test/mocha/version.test.js",
    "ui-test": "yyt ./test/yyt",
    "cga": "yyl watch --proxy --config ./test/case/gulp-requirejs/config.js"
  },
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/yyl.git"
  },
  "bugs": {
    "url": "https://github.com/jackness1208/yyl/issues"
  },
  "homepage": "https://github.com/jackness1208/yyl#readme",
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "init-me": "0.4.4",
    "inquirer": "6.2.0",
    "keypress": "*",
    "node-watch": "0.6.2",
    "yyl-fs": "0.3.2",
    "yyl-hander": "0.6.1",
    "yyl-os": "0.8.3",
    "yyl-print": "0.7.3",
    "yyl-request": "0.1.1",
    "yyl-seed-gulp-requirejs": "4.0.0-beta2",
    "yyl-seed-webpack": "2.0.0-beta2",
    "yyl-server": "0.2.0",
    "yyl-util": "2.3.3"
  },
  "devDependencies": {
    "chai": "~4.1.2",
    "mocha": "^7.0.0",
    "yyl-file-replacer": "^0.4.1",
    "yyl-seed-test-util": "^0.3.1"
  }
}
