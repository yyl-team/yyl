{
  "name": "yyl",
  "version": "3.11.0",
  "description": "yy live workflow",
  "keywords": [
    "yylive"
  ],
  "preferGlobal": true,
  "author": "yylive FE team",
  "license": "ISC",
  "bin": {
    "yyl": "bin/init.js"
  },
  "eslintConfig": {
    "extends": [
      "yyl-node"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "/test/case",
    "yyt",
    "__frag",
    "__gitcase"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "endOfLine": "lf",
    "quoteProps": "consistent"
  },
  "scripts": {
    "prepublishOnly": "npm run prettier & npm test",
    "eslint": "eslint ./",
    "eslint:fix": "eslint ./ --fix",
    "prettier": "prettier --write **/*.js",
    "test": "mocha ./test/mocha --timeout 10000000",
    "test:all.git.yycom": "mocha ./test/mocha/all.git.yycom.test.js --timeout 1000000",
    "test:all.git.yycom_header": "mocha ./test/mocha/all.git.yycom_header.test.js --timeout 1000000",
    "test:all.git.yycom_h5_live": "mocha ./test/mocha/all.git.yycom_h5_live.test.js --timeout 1000000",
    "test:all.git.usercenter": "mocha ./test/mocha/all.git.usercenter.test.js --timeout 1000000",
    "test:all": "mocha ./test/mocha/all.test.js --timeout 1000000",
    "test:help": "mocha ./test/mocha/help.test.js --timeout 1000000",
    "test:info": "mocha ./test/mocha/info.test.js --timeout 1000000",
    "test:init": "mocha ./test/mocha/init.test.js --timeout 1000000",
    "test:mock": "mocha ./test/mocha/mock.test.js --timeout 1000000",
    "test:path": "mocha ./test/mocha/path.test.js --timeout 1000000",
    "test:server": "mocha ./test/mocha/server.test.js --timeout 1000000",
    "test:version": "mocha ./test/mocha/version.test.js --timeout 1000000",
    "ui-test": "yyt ./test/yyt --timeout 1000000",
    "cgw": "yyl watch --proxy --config ./test/case/gulp-requirejs/config.js",
    "cww": "yyl watch --proxy --config ./test/case/gulp-requirejs/config.js",
    "crw": "yyl watch --proxy --config ./test/case/react-ts/yyl.config.js",
    "cra": "yyl all --config ./test/case/react-ts/yyl.config.js --isCommit",
    "cec": "yyl all --proxy --config ./test/case/err-config/yyl.config.js",
    "ceo": "yyl all --proxy --config ./test/case/err-optimize/yyl.config.js",
    "guw": "yyl watch --proxy --config ./test/__gitcase/usercenter/config.js --name pc --logLevel 2"
  },
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackness1208/yyl.git"
  },
  "bugs": {
    "url": "https://github.com/jackness1208/yyl/issues"
  },
  "homepage": "https://github.com/jackness1208/yyl#readme",
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "init-me": "0.4.7",
    "inquirer": "6.2.0",
    "keypress": "*",
    "yyl-fs": "0.3.2",
    "yyl-hander": "0.10.0",
    "yyl-os": "0.11.0",
    "yyl-print": "0.7.3",
    "yyl-request": "0.1.1",
    "yyl-seed-gulp-requirejs": "4.6.0",
    "yyl-seed-webpack": "2.3.1",
    "yyl-server": "0.3.6",
    "yyl-util": "2.3.8"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "eslint": "6.8.0",
    "eslint-config-yyl-node": "^0.1.1",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "7.0.0",
    "prettier": "^2.0.2",
    "yyl-file-replacer": "0.4.1",
    "yyl-seed-test-util": "0.3.1"
  }
}
